{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@angular/*": ["node_modules/@angular/*"],
      "ui": [
        "dist/ui"
      ],
      "core": [
        "dist/ui/core/core",
        "dist/ui/core"
      ],
      "banner": [
        "dist/ui/feedback/banner/banner",
        "dist/ui/feedback/banner"
      ],
      "button": [
        "dist/ui/button/button",
        "dist/ui/button"
      ],
      "local-nav": [
        "dist/ui/navigation/local-nav/local-nav",
        "dist/ui/navigation/local-nav"
      ],
      "switcher": [
        "dist/ui/navigation/switcher/switcher",
        "dist/ui/navigation/switcher"
      ],
      "nav-toggle": [
        "dist/ui/navigation/toggle/toggle",
        "dist/ui/navigation/toggle"
      ],
      "tabs": [
        "dist/ui/navigation/tabs/tabs",
        "dist/ui/navigation/tabs"
      ],
      "mega-menu": [
        "dist/ui/navigation/mega-menu/mega-menu",
        "dist/ui/navigation/mega-menu"
      ],
      "form-field": [
        "dist/ui/form/form-field/form-field",
        "dist/ui/form/form-field"
      ],
      "input": [
        "dist/ui/form/input/input",
        "dist/ui/form/input"
      ],
      "badge": [
        "dist/ui/badge/badge",
        "dist/ui/badge"
      ],
      "avatar": [
        "dist/ui/avatar/avatar",
        "dist/ui/avatar"
      ],
      "snackbar": [
        "dist/ui/snackbar/snackbar",
        "dist/ui/snackbar"
      ],
      "faq": [
        "dist/ui/faq/faq",
        "dist/ui/faq"
      ],
      "page-nav": [
        "dist/ui/navigation/page-nav/page-nav",
        "dist/ui/navigation/page-nav"
      ],
      "paginator": [
        "dist/ui/paginator/paginator",
        "dist/ui/paginator"
      ],
      "checkbox": [
        "dist/ui/form/checkbox/checkbox",
        "dist/ui/form/checkbox"
      ],
      "list": [
        "dist/ui/navigation/list/list",
        "dist/ui/navigation/list"
      ],
      "slide-over": [
        "dist/ui/slide-over/slide-over",
        "dist/ui/slide-over"
      ],
      "select": [
        "dist/ui/form/select/select",
        "dist/ui/form/select"
      ],
      "table": [
        "dist/ui/table/table",
        "dist/ui/table"
      ],
      "sort": [
        "dist/ui/sort/sort",
        "dist/ui/sort"
      ],
      "stepper": [
        "dist/ui/stepper/stepper",
        "dist/ui/stepper"
      ],
      "panel": [
        "dist/ui/layout/panel/panel",
        "dist/ui/layout/panel"
      ],
      "dialog": [
        "dist/ui/feedback/dialog/dialog",
        "dist/ui/feedback/dialog"
      ],
      "loader": [
        "dist/ui/loader/loader",
        "dist/ui/loader"
      ],
      "toast": [
        "dist/ui/feedback/toast/toast",
        "dist/ui/feedback/toast"
      ],
      "radio": [
        "dist/ui/form/radio/radio",
        "dist/ui/form/radio"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
